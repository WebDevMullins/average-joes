<section class='flex flex-col items-center px-6 py-8 mx-auto md:h-screen max-w-screen-xl lg:py-0'>
	<div class='w-full max-w-sm bg-white border border-gray-200 rounded-lg shadow dark:bg-gray-800 dark:border-gray-700'>
		<div class='flex flex-col items-center py-10'>
			<img class='w-24 h-24 mb-3 rounded-full shadow-lg' src='/images/gym.png' alt='Profile Image' />
			<h5 class='mb-1 text-xl font-medium text-gray-900 dark:text-white'>{{f_name}} {{l_name}}</h5>
			<span class='text-sm text-gray-500 dark:text-gray-400'>{{plan.name}}</span>
			<span class='text-sm text-gray-500 dark:text-gray-400'>{{tier.name}}</span>
			<span class='text-sm text-gray-500 dark:text-gray-400'>{{tier.benifits}}</span>
			<span class='text-sm text-gray-500 dark:text-gray-400'>{{tier.trainer.f_name}} {{tier.trainer.l_name}}</span>
			<span class='text-center text-sm text-gray-500 dark:text-gray-400'>{{tier.trainer.certifications}}</span>
		</div>
	</div>
</section>